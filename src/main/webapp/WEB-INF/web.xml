        <servlet-name>AlumnoPostulacionesServlet</servlet-name>
        <url-pattern>/alumno/alumno_postulaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FamiliaAntecedenteServlet</servlet-name>
        <url-pattern>/familia/familia_antecedente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarAlumnosRegistradosServlet</servlet-name>
        <url-pattern>/cem/actualizar_alumnos_registrados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlumnoCalificacionServlet</servlet-name>
        <url-pattern>/alumno/calificaciones</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
    <error-page>
        <error-code>404</error-code>
        <location>/error/no-encontrado.html</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/error/error-servidor.html</location>
    </error-page>
</web-app>
