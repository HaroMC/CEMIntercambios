        <servlet-class>cem.intercambios.controlador.servlet.CemProgramaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CemAlumnoServlet</servlet-name>
        <servlet-class>cem.intercambios.controlador.servlet.CemAlumnoServlet</servlet-class>
