    <servlet>
        <servlet-name>ActualizarAlumnosRegistradosServlet</servlet-name>
        <servlet-class>cem.intercambios.controlador.servlet.ActualizarAlumnosRegistradosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlumnoCalificacionServlet</servlet-name>
        <servlet-class>cem.intercambios.controlador.servlet.AlumnoCalificacionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IniciarSesionServlet</servlet-name>
        <url-pattern>/ingresar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesionServlet</servlet-name>
        <url-pattern>/salir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarCuentaServlet</servlet-name>
        <url-pattern>/registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CelPostulacionesServlet</servlet-name>
        <url-pattern>/cel/inscripciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InicializarPerfil</servlet-name>
        <url-pattern>/inicializar-perfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CemProgramaServlet</servlet-name>
        <url-pattern>/cem/cem_programas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CemAlumnoServlet</servlet-name>
        <url-pattern>/cem/cem_alumnos</url-pattern>
