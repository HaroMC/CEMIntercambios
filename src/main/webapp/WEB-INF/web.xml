        <servlet-name>CemInscripcionAlumnoServlet</servlet-name>
        <url-pattern>/cem/inscripciones_alumnos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CemFamiliaServlet</servlet-name>
        <url-pattern>/cem/cem-administrar_familias</url-pattern>
